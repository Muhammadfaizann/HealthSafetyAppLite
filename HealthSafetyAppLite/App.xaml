<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HealthSafetyAppLite.App">
    <Application.Resources>
        <Color x:Key="HsGreen">#00B04E</Color>
        <Color x:Key="HsBlack">#626262</Color>
        <Color x:Key="HsRed">#FC0301</Color>
        <Color x:Key="HsYellow">#F4F212</Color>



        <OnPlatform x:Key="FontAwesomeSolid" x:TypeArguments="x:String">
            <On Platform="Android" Value="PoppinsRegular" />
            <On Platform="iOS" Value="PoppinsRegular" />
        </OnPlatform>
    </Application.Resources>
</Application>